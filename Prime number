


public class Narala{
    public static void main(String[] args){
        int n=4;
        if(isprime(n))
        System.out.print("prime");
        else
        System.out.print("not a prime");
    }
    public static boolean isprime(int n){
        for(int i=2;i<=n/2;i++){
            if(n%i==0){
                return false;
            }
        }
        return true;


        }
    }

   
    
   
       
      
    
   
  


   
       
   



       
